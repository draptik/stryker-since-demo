{
  "stryker-config": {
    "concurrency": 8,
    "mutation-level": "Standard",
    "mutate": [
      "**/*"
    ],
    "solution": "Demo.sln",
    "project": "Demo.csproj",
    "coverage-analysis": "perTest",
    "disable-bail": false,
    "disable-mix-mutants": false,
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "verbosity": "info",
    "reporters": [
      "Progress",
      "Html"
    ],
    "since": {
      "enabled": true,
      "ignore-changes-in": [],
      "target": "v1"
    },
    "test-projects": [],
    "test-case-filter": "",
    "ignore-mutations": [],
    "ignore-methods": [],
    "report-file-name": "mutation-report",
    "break-on-initial-test-failure": false
  }
}
